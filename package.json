		"vitepress": "1.0.0-alpha.28",
		"vitepress-theme-demoblock": "^2.0.0",
		"vitest": "^0.25.2",
		"vue-tsc": "^1.0.9"
	},
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"webpack"
			]
		}
	},
	"vetur": {
