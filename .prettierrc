printWidth: 80
tabWidth: 2
useTabs: true
semi: false
singleQuote: true
quoteProps: 'as-needed'
trailingComma: 'none'
bracketSpacing: true
jsxBracketSameLine: false
jsxSingleQuote: true
arrowParens: 'always'
eslintIntegration: true
newlinePerChainedCall: true
requirePragma: false
insertPragma: false
proseWrap: 'preserve'
htmlWhitespaceSensitivity: 'ignore'
vueIndentScriptAndStyle: false
endOfLine: 'lf'
embeddedLanguageFormatting: 'auto'